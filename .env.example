# YesNoApp Environment Variables
# Copy this file to .env and fill in your values

# Database (Required)
DATABASE_URL=postgresql://username:password@localhost:5432/yesnoapp

# AI Analysis (Required for food analysis)
GOOGLE_GEMINI_API_KEY=your_gemini_api_key_here

# Authentication (Required)
SESSION_SECRET=your_random_session_secret_here

# Payment Processing (Optional - for subscriptions)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key

# Email Services (Optional - for password recovery)
SENDGRID_API_KEY=SG.your_sendgrid_api_key

# Analytics (Optional)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Automatic Environment Variables (Do not modify)
PGDATABASE=
PGHOST=
PGPASSWORD=
PGPORT=
PGUSER=
REPLIT_DOMAINS=
REPL_ID=
